# KeaBot Backend Environment
# Copy to .env and fill in your values

# LLM Provider: "gemini" or "openai"
LLM_PROVIDER=gemini

# API Keys (set the one you're using)
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Model names (optional - defaults provided)
GEMINI_MODEL=gemini-2.0-flash
OPENAI_MODEL=gpt-4o

# Safety settings
KEABOT_ALLOWED_PATHS=f:/sistemas/KeaBOT
KEABOT_SAFETY_MODE=strict

# Database
DATABASE_URL=sqlite+aiosqlite:///./keabot.db
